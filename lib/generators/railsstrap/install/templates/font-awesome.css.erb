@font-face {
   font-family:'FontAwesome';
   src:    font-url('fontawesome/fonts/fontawesome-webfont.eot');
   src:    font-url('fontawesome/fonts/fontawesome-webfont?#iefix') format('embedded-opentype'),
   font-url('fontawesome/fonts/fontawesome-webfont.woff') format('woff'),
   font-url('fontawesome/fonts/fontawesome-webfont.ttf') format('truetype'),
   font-url('fontawesome/fonts/fontawesome-webfont.svg') format('svg');
   font-weight:normal;
   font-style:normal;
}